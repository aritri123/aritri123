void plusMinus(int arr_count, int* arr) {
int i;
float a;
float b;
float c;
float u=0;
float v=0;
 float w=0;
for(i=0;i<arr_count;i++)
{
  if(arr[i]==0)  
  u++;
  if(arr[i]<0)
   v++;
  if(arr[i]>0)
  w++;
}
a=u/arr_count;
b=v/arr_count;
c=w/arr_count;
printf("%.6f",c);
printf("\n%.6f",b);
printf("\n%.6f",a);
}
