int birthday(int s_count, int* s, int d, int m) 
{
    int i;
   int k ;
  int sum;
    int count=0;
    for(i=0;i<s_count;i++)
    {
        sum=0;
      
          for(int k=i;k<(i+m);k++)
          {
              
          
          sum=sum+s[k];
      }
      
      if (sum==d)
      {
        ++count;
      }
    }
    return count; }
