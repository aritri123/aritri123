int* compareTriplets(int a_count, int* a, int b_count, int* b, int* result_count)
 {

    
    int i;
    int j;
    int m=0;
    
    int n=0;
    for( i=0;i<a_count;i++)
    {
        if(a[i]>b[i])
     m++;
        else if (a[i]<b[i])
       n++;
    else
    {
        m=m+0;
        n=n+0;
    }
      
    }
  for(j=0;j<=1;j++)
  {
      if(j==0)
      result_count[j]=m;
      else if(j==1)
      result_count[j]=n;
  }
  return  result_count;
}
   
