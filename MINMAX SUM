void miniMaxSum(int arr_count, int* arr) {
    int min;
    int max,sum=0;
    min =arr[0];
    max=arr[0];
    int i;
    int w;
    int x;
    for (i=0;i<arr_count;i++)
    {
        sum=sum+arr[i];
        if(arr[i]<min)
        {
            min=arr[i];
        }
        if(arr[i]>max)
        {
            max=arr[i];
        }
        
    }
    w=sum-max;
    x=sum-min;
    printf("%d\t%d",w,x);
    }

